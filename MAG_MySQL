# Affiliations.text

CREATE TABLE affiliations (
    AffiliationId long,
    mRank int,
    NormalizedName text,
    DisplayName text,
    GridId text,
    OfficialPage text,
    WikiPage text,
    PaperCount double,
    CitationCount double,
    CreatedDate text
  #  PRIMARY KEY (AffiliationId)
);

LOAD DATA INFILE 'C:/Users/aweki/Downloads/affiliations.csv' 
INTO TABLE affiliations 
FIELDS TERMINATED BY ',' 
ENCLOSED BY '"'
LINES TERMINATED BY '\n'
IGNORE 1 ROWS;
